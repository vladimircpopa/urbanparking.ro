<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Reservation" table="reservation">
    <indexes>
      <index name="user_id" columns="user_id"/>
      <index name="parking_slot_id" columns="parking_slot_id"/>
      <index name="status" columns="status"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="startTime" type="datetime" column="start_time" nullable="false"/>
    <field name="endTime" type="datetime" column="end_time" nullable="false"/>
    <field name="availability" type="boolean" column="availability" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <many-to-one field="user" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="parkingSlot" target-entity="ParkingSlot" fetch="LAZY">
      <join-columns>
        <join-column name="parking_slot_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
